    "dev": "webpack-dev-server --hot --inline --progress --content-base public",
    "dev2": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test:watch": "npm run test -- --watch",
    "tw": "npm run test:watch",
    "pack": "webpack --watch",
    "scss": "scss --watch app/style/scss:public/styles",
    "build": "npm install && scss --update app/style/scss:public/styles && webpack && nodemon",
    "prepublish": "npm test && webpack && nodemon"
  },
